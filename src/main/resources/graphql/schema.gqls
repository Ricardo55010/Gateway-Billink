type UserDTO {
    id: ID!
    name: String
}

type BudgetDTO {
    id: ID!
    title: String
    id: userId
}

# The Root Query for the application
type Query {
    getUser(id: Int): UserDTO!
    getBudget(userId: Int): BudgetDTO!
}

# The Root Mutation for the application
type Mutation {
    createUser(name: String!) : UserDTO!
    createBudget(title: String!); BudgetDTO!
}